{{#> layout}}
    <div class="jw-tree-node jw-element jw-image-text jw-node-is-first-child">
        <div class="jw-element-imagetext-text">
            <h2 class="jw-heading-100"><span style="color: #202020;"></span></h2>
            <h2 class="jw-heading-100"><span style="color: #202020;"></span></h2>
            <h2 class="jw-heading-100"><span style="color: #202020;"><strong>Maak kennis met de beelden van&nbsp;Harry van de Ven.</strong></span>
            </h2>
            <p>&nbsp;</p>
            <p>&nbsp;</p>
            <p>&nbsp;</p>
        </div>
    </div>
    <div class="jw-tree-node jw-element jw-columns jw-tree-container jw-tree-horizontal jw-columns--mode-flex">
        <div style="width: 50%"
             class="jw-tree-node jw-element jw-column jw-tree-container jw-node-is-first-child jw-responsive lt400 lt480 lt540 lt600 lt640 lt800">
            <div class="jw-tree-node jw-element jw-image jw-node-is-first-child jw-node-is-last-child">
                <div class="jw-intent jw-element-image jw-element-child jw-element-content jw-element-activeless jw-element-no-margin  jw-element-image-is-left"
                     style="width: 100%;">
                    <picture class="jw-element-image__image-wrapper jw-image-is-square jw-intrinsic"
                             style="padding-top: 90.459363957597%;">
                        <img class="jw-element-image__image jw-intrinsic__item" width="283" height="256"
                             style="--jw-element-image--pan-x: 1; --jw-element-image--pan-y: 0.5" alt=""
                             src="images/hout.webp"
                             sizes="(max-width: 640px) 100vw, (max-width: 590px) 48vw, 283px" loading="lazy">
                    </picture>
                </div>
            </div>
        </div>
        <div style="width: 50%"
             class="jw-tree-node jw-element jw-column jw-tree-container jw-node-is-last-child jw-responsive lt400 lt480 lt540 lt600 lt640 lt800">
            <div class="jw-tree-node jw-element jw-image jw-node-is-first-child jw-node-is-last-child">
                <div class="jw-intent jw-element-image jw-element-child jw-element-content jw-element-activeless jw-element-no-margin  jw-element-image-is-left"
                     style="width: 192px;">
                    <picture class="jw-element-image__image-wrapper jw-image-is-square jw-intrinsic"
                             style="padding-top: 133.33333333333%;">
                        <img class="jw-element-image__image jw-intrinsic__item" width="192" height="256"
                             style="--jw-element-image--pan-x: 0.5; --jw-element-image--pan-y: 0.5" alt=""
                             src="images/serpentijn.webp"
                             sizes="(max-width: 640px) 192px, (max-width: 590px) 48vw, 192px" loading="lazy">
                    </picture>
                </div>
            </div>
        </div>
    </div>
    <div class="jw-tree-node jw-element jw-columns jw-tree-container jw-tree-horizontal jw-columns--mode-flex jw-node-is-last-child">
        <div style="width: 50%"
             class="jw-tree-node jw-element jw-column jw-tree-container jw-node-is-first-child jw-responsive lt400 lt480 lt540 lt600 lt640 lt800">
            <div class="jw-tree-node jw-element jw-image-text jw-node-is-first-child jw-node-is-last-child">
                <div class="jw-element-imagetext-text">
                    <h1 class="jw-heading-130"><span style="color: #202020;"><strong>Beelden in hout </strong></span>
                    </h1>
                    <h1 class="jw-heading-130"><span style="color: #202020;"><strong>(Eik).</strong></span></h1>
                </div>
            </div>
        </div>
        <div style="width: 50%"
             class="jw-tree-node jw-element jw-column jw-tree-container jw-node-is-last-child jw-responsive lt400 lt480 lt540 lt600 lt640 lt800">
            <div class="jw-tree-node jw-element jw-image-text jw-node-is-first-child jw-node-is-last-child">
                <div class="jw-element-imagetext-text">
                    <h1 class="jw-heading-130"><span
                            style="color: #202020;"><strong>Beelden in steen</strong></span></h1>
                    <h1 class="jw-heading-130"><span style="color: #202020;"><strong> (Serpentijn).</strong></span>
                    </h1></div>
            </div>
        </div>
    </div>
{{/layout}}
